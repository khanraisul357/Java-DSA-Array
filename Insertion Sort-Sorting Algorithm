-->In Ascending Order

package sorting_algorithm.insertion_sort.ascending_order;

class InsertionSortInAscendingOrder {

	
	//In best case--> Time Complexity= O(n) & In average/worst case--> Time Complexity= O(n^2)
	public static void insertionSort(int[] arr,int size) {
		
		for(int i=1;i<size;i++) {
			
			int insertingElement=arr[i];
			int prev=i-1;
			
			while(prev>=0 && arr[prev] >insertingElement) {				
				
				arr[prev+1]=arr[prev];			
				prev--;
				
			}
			
			arr[prev+1]=insertingElement;
			
		}
		
		for(int a:arr){
			System.out.print(a+" ");
		}
		
	}

}



public class Main {

	public static void main(String[] args) {
		
		int[] array= {16,8,2,10,4,13};
		int size=array.length;
		
		InsertionSortInAscendingOrder.insertionSort(array,size);

	}

}





-->In Ascending Order
